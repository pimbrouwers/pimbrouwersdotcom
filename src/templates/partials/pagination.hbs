<nav class="pagination{{#if (or prevUrl nextUrl)}}{{else}} pagination-none{{/if}}">
  <ul>
    <li class="previous{{#if nextUrl}}{{else}} disabled{{/if}}">
    	<a{{#if nextUrl}} href="{{nextUrl}}" data-total-pages="{{totalPages}}"{{else}} href="#"{{/if}}>
	    	&laquo; Older Posts
	    </a>
	</li>
    <li class="next{{#if prevUrl}}{{else}} disabled{{/if}}">
    	<a{{#if prevUrl}} href="{{prevUrl}}" data-total-pages="{{totalPages}}"{{else}} href="#"{{/if}}>Newer Posts
	    	&raquo;
	    </a>
	</li>
  </ul>
</nav>
